{
  "name": "vue-auth-cognito",
  "version": "0.1.5",
  "description": "A Vue.js Vuex module for authentication using Amazon AWS Cognito",
  "main": "lib/index.js",
  "scripts": {
    "test": "tap -Rspec --coverage --coverage-report=html test/unit/index.js",
    "test:watch": "tape-watch test/unit/index.js -p tap-spec",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "coverage": "tap --cov --coverage-report=html --strict test/unit/index.js"
  },
  "repository": "https://github.com/kopterio/vue-auth-cognito",
  "author": "Evgeny Zislis <evgeny.zislis@gmail.com>",
  "contributors": [
    {
      "name": "Anton Sekatski",
      "email": "antonsekatski@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "amazon-cognito-identity-js": "^1.19.0",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bower": "^1.8.0",
    "codeclimate-test-reporter": "^0.5.0",
    "cov": "^0.1.3",
    "coveralls": "^3.0.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "proxyquire": "^1.8.0",
    "sinon": "^4.0.0",
    "tap": "^11.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0",
    "tape-watch": "^2.3.0",
    "vue": "^2.4.1"
  },
  "release-script": {
    "defaultDryRun": "true",
    "bowerRepo": "git@github.com:kopterio/vue-auth-cognito-bower.git"
  }
}
